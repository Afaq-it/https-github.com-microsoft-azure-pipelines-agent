{
    "version": "1.0.0-*",   
    "compilationOptions": {
        "emitEntryPoint": true,
        "warningsAsErrors": true
    },
    
    "configurations": {
        "Debug": {
            "compilationOptions": {
                "define": [
                    "DEBUG",
                    "TRACE",
                    "OS_WINDOWS"
                ]
            }
        },
        "Release": {
            "compilationOptions": {
                "define": [
                    "TRACE",
                    "OS_WINDOWS"
                ],
                "optimize": true
            }
        }
    },
    
    "dependencies": {
        "NETStandard.Library": "1.0.0-rc3-23721",
        "Microsoft.NETCore.TestHost": "1.0.0-rc3-23721",
        "moq.netcore": "4.4.0-beta8",
        "xunit": "2.1.0",
        "xunit.console.netcore": "1.0.2-prerelease-00101",
        "xunit.netcore.extensions": "1.0.0-prerelease-00153",
        "xunit.runner.utility": "2.1.0",
        "Agent.Listener": { "target": "project", "version": "*" },
        "vss-api-netcore": "0.5.2-private",
        "Microsoft.VisualStudio.Services.Agent": { "target": "project", "version": "*" },
        "Agent.Worker": { "target": "project", "version": "*" } 
    },
    "frameworks": {
        "dnxcore50": {
            "imports": "portable-net451+win8"
        }
    }
}
